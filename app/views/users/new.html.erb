<div class="login">
  <div class="container">
    <div class="form">

    <h1>Sign up</h1>
      <%= form_for @user do |f| %>
        <%= f.label :name %>:
        <%= f.text_field :name %><br />

        <%= f.label :title %>:
        <%= f.text_field :title %><br />
      	
      	<%= f.label :email %>:
        <%= f.email_field :email %><br />
			
        <%= f.label :passowrd %>:
        <%= f.password_field :password %><br />

        <%= f.submit %>
      <% end %>


  
    </div>
  </div>
</div>
